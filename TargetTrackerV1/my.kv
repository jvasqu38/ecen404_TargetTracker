WindowManager:
    StartWindow:
    MainWindow:

<StartWindow>:
    name: "start"

    GridLayout:
        cols:1

        GridLayout:
            cols:2

            Label:
                text: "pass"

            TextInput:
                id: passq
                multiline: False
            Button:
                text: "Start"
                on_press: app.root.current = "main"
                on_release: root.pressed()

<MainWindow>:
    name: "main"
    GridLayout:
        rows:2
        size: root.width, root.height

        GridLayout:
            cols:3
            height: root.height*1.2
            Image:
                id: Before
                source: 'image2_set1.jpg'



            Image:
                id: After
                source: 'image3_set1.jpg'


            Image:
                id: After
                source: 'image3_set1.jpg'


        GridLayout:
            cols:2
            height: root.height*.8
            Button:
                text: "switch target"

            Button:
                text: "End"










